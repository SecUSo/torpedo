{
  "manifest_version": 2,
  "author": "SECUSO",
  "applications": {
    "gecko": {
      "id": "torpedo@tu-darmstadt.de"
    }
  },
  "name": "__MSG_extensionName__",
  "version": "3.2.2",
  "default_locale": "de",
  "description": "__MSG_extensionDescription__",
  "background": {
    "scripts": [
      "js/jquery-3.4.1.min.js",
      "js/punycode.min.js",
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "messagesModify"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "38": "img/icon38.png",
    "64": "img/icon64.png"
  },
  "web_accessible_resources": [
    "img/*.png",
    "img/*.svg"
  ]
}